{
    "nodes": [
        {
            "node": "Gene",
            "example": [
                "COPB2",
                "STAT3"
            ],
            "display": [
                {
                    "label": "Gene ID",
                    "text": "${id}",
                    "type": "text"
                },
                {
                    "label": "label",
                    "text": "${label}",
                    "type": "text"
                },
                {
                    "label": "uri",
                    "text": "${uri}",
                    "href": "${uri}",
                    "type": "link"
                }
            ],
            "relation": [],
            "search": [
                "id",
                "label"
            ]
        },
